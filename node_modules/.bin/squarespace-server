#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/echo" ]; then
  exec "$basedir/echo" off & "$basedir/../@squarespace/server/build/distributions/local-developer/bin/run.bat" "$@"
else 
  exec echo off & "$basedir/../@squarespace/server/build/distributions/local-developer/bin/run.bat" "$@"
fi
